varA=triArea(3,2);
varB=triArea(7,4);
varC=triArea(10,10);



function triArea(base,height){
return base * height/2
}

function FirstReverse(str) { 


// code goes here  

let FirstReverse = str.split("").map(word => str.split("").reverse().join());
return FirstReverse;
}

// keep this function call here 
console.log(FirstReverse(readline()));